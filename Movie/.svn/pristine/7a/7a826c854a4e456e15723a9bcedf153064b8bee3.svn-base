<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>회원정보 페이지</title>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
<script src="js/member_info.js?ver=08"></script>
<link rel="stylesheet" href="css/member/member_info.css?ver=09">
</head>
<body>

<div id="container">
	<table id="memberInfoTable">
		<tr>
			<td colspan="2"><label>회원정보 입니다</label><hr></td>
			
		</tr>
		<tr>
			<th><label>아이디</label></th>
			<td id="memberId">${info.memberId } </td>
		</tr>
		<tr>
			<th><label>비밀번호</label></th>
			<td>${info.memberPassword }  </td>
		</tr>
		<tr>
			<th><label>이 름</label></th>
			<td>${info.memberName } </td>
		</tr>
		<tr>
			<th><label>전화번호</label></th>
			<td>${info.memberTel } </td>
		</tr>
		<tr>
			<td><label>포인트</label></td>
			<td>${info.memberPoint } P </td>
		</tr>
		<tr>
			<td colspan="2">
				<input type="button" value="수정하기" id="updateBtn"/>
			</td>
		</tr>
	
	</table>
</div>
	

</body>
</html>