<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>회원가입 페이지</title>
</head>
<body>
<form action="joinResult.do" method="post">
	<table border="1">
		<tr>
			<td>아이디</td>
			<td><input type="text" name="memberId"/><input type="button" value="중복확인"/></td>
		</tr>
		<tr>
			<td>비밀번호</td>
			<td><input type="password" name="memberPassword"/></td>
		</tr>
		<tr>
			<td>이름</td>
			<td><input type="text" name="memberName"/></td>
		</tr>
		<tr>
			<td>TEL</td>
			<td><input type="tel" name="memberTel"/></td>
		</tr>
		<tr>
			<td colspan="2" align="center"><input type="submit" value="가입"/></td>
		</tr>		
	</table>

</form>

</body>
</html>