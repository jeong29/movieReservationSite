<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>관리자 페이지</title>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
<script src="js/admin_page_list.js?ver=02"></script>
<style type="text/css">
/* #insertMovieBtn, #selectMember, #listBtn {
	margin: 10px auto;
	width: 100px;
	height: 35px;
	font-weight: bold;
	color: white;
	background-color: #F24535;
	border-style: none;
} */
#adminDiv {
	margin: 20px auto;
}

#adminTable {
	border: 1px solid black;
	border-collapse: collapse;
	margin: 10px auto;
	width: 90%;
	height: 500px;
}

#adminTable td {
	border: 1px solid black;
	padding: 5px;
}
#selectorList {
	width: 100%;
	height: 100%;
}
#memberListTable {
	border: 1px solid black;
	border-collapse: collapse;
	margin: 0 auto;
	width: 90%;
}
#memberListTable td {
	border: 1px solid black;
}
</style>
</head>
<body>
	<div id="adminDiv">
		<table id="adminTable">
			<colgroup>
				<col width="20%">
				<col width="*">
			</colgroup>
			<tr>
				<td>
					<select id="selectorList" multiple>
						<optgroup label="회원관리">
							<option id="memberList">회원정보조회</option>
						</optgroup>
						<optgroup label="영화관리">
							<option id="insertMovie">영화등록</option>
							<option id="movieList">영화목록</option>
						</optgroup>
					</select>
				</td>
				<td>
					<div id="ListDiv">
					<input type="hidden" value="${sessionScope.MEMBER_INFO.memberId }" id="memberId">
					<!-- 회원 정보 조회 -->
					<!-- 영화 목록 조회 -->
					</div>
				</td>
			</tr>
		</table>
		<input type="button" value="목록가기" id="listBtn" />







<%-- <input type="button" value="회원정보조회" id="selectMember" /><br>
<input type="button" value="영화등록" id="insertMovieBtn" /><br> --%>
	</div>
</body>
</html>